{
  "name": "pretty-prism-back-end",
  "version": "0.1.0",
  "description": "PrettyPrism: Back end",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "",
  "author": "",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^1.0.4",
    "apollo-upload-server": "^4.0.0-alpha.1",
    "aws-sdk": "^2.155.0",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.13.3",
    "chalk": "^2.3.0",
    "cors": "^2.7.1",
    "dataloader": "^1.3.0",
    "dotenv": "^4.0.0",
    "express": "^4.13.3",
    "graphql": "0.10.5",
    "graphql-subscriptions": "^0.4.4",
    "graphql-tools": "^1.0.0",
    "jsonwebtoken": "^8.1.0",
    "moment": "^2.19.2",
    "mongodb": "^2.2.29",
    "morgan": "^1.8.0",
    "subscriptions-transport-ws": "^0.7.3"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^3.1.1",
    "eslint-config-node": "^1.6.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.9.2"
  }
}
